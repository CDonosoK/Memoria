	note = {Publisher: ILADES. Universidad Alberto Hurtado.},
	pages = {3--49},
	file = {Full Text PDF:C\:\\Users\\cleme\\Zotero\\storage\\L9ZKNTRX\\Rivera Taiba y Rivera Taiba - 2019 - Efectos de la automatización en el empleo en Chile.pdf:application/pdf},
